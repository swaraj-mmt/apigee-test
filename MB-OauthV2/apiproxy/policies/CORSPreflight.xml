<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault enabled="true" continueOnError="false" async="false" name="CORSPreflight">
    <DisplayName>CORSPreflight</DisplayName>
    <FaultRules/>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers>
                <Header name='Access-Control-Allow-Origin'>*</Header>
                <Header name='Access-Control-Allow-Methods'>POST, GET, OPTIONS</Header>
                <Header name='Access-Control-Allow-Headers'>Accept, Content-Type, connection, content-length, Authorization</Header>
                <Header name='Access-Control-Allow-Credentials'>true</Header> 
            </Headers>
            
            <StatusCode>200</StatusCode>
            <ReasonPhrase>CORS Preflight</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>


