<ExtractVariables enabled="true" continueOnError="true" async="false" name="ExtractSessionVariables">
    <DisplayName>ExtractSessionVariables</DisplayName>
    <FaultRules/>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Source clearPayload="false">session.Payload</Source>
    <JSONPayload > 
		 <Variable name="firstname">  
			<JSONPath>$.firstName</JSONPath>
		</Variable>
		 <Variable name="lastname">
			<JSONPath>$.lastname</JSONPath>
		</Variable>
		<Variable  name="msisdn">
			<JSONPath>$.msisdn</JSONPath>
		</Variable>
		<Variable  name="callBackUrl">
			<JSONPath>$.callBackUrl</JSONPath>
		</Variable>
    </JSONPayload>
    </ExtractVariables>

