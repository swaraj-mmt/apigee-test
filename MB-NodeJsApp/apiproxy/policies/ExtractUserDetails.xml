<ExtractVariables name="ExtractUserDetails">
	<Source>request</Source>
	<IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
	<FormParam name="msisdn">
		<Pattern>{create_msisdn}</Pattern>
	</FormParam>
	<FormParam name="name">
		<Pattern>{create_name}</Pattern>
	</FormParam>
	<FormParam name="lastName">
		<Pattern>{create_lastName}</Pattern>
	</FormParam>
	<FormParam name="username">
		<Pattern>{create_username}</Pattern>
	</FormParam>
	<FormParam name="password">
		<Pattern>{create_password}</Pattern>
	</FormParam>
</ExtractVariables>
